{
  "AppSettings": {
    "ApiUrl": "https://localhost:5001"
  },
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "Filter": {
      "0": {
        "Name": "ByExcluding",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
        }
      }
    }
  }
}
